{
  "name": "Esp32React",
  "displayName": "Esp32React",
  "expo": {
    "name": "react-native-esp32",
    "slug": "react-native-esp32",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/react-logo.png",
    "splash": {
      "image": "./assets/react-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#dddddd"
    },
    "primaryColor": "#dddddd",
    "privacy": "public",
    "androidNavigationBar": {
      "barStyle": "light-content",
      "visible": "sticky-immersive"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}